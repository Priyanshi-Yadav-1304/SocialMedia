<%- include('partials/header')%>
<div class="loginbox">
    <form action="/create-post" method="POST" enctype="multipart/form-data">
        <label>Title</label>
        <input name="title" type="text" placeholder="Title">
        <label>Content</label>
        <input name="content" type="text" placeholder="Content">
        <label>Image</label>
        <input class="fileinput" name="avatar" type="file" >
        <p hidden class="postpara">Please provide image</p>
        <label></label>
        <input class="sb" type="submit" >
        <input hidden class="tb" type="submit" >
    </form>

</div>
<script src="/javascripts/post.js"></script>


